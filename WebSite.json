{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {

        "siteName": {
            "type": "string",
            "defaultValue": "WebApp-AzureUC"

        },
        "hostingPlanName": {
            "type": "string",
            "defaultValue": "WebApp-AzureUC-HP"
        },
        "siteLocation": {
            "type": "string",
            "defaultValue": "West US"
        },
        "sku": {
            "type": "string",
            "allowedValues": [
                "Free",
                "Shared",
                "Basic",
                "Standard",
                "Premium"
            ],
            "defaultValue": "Standard"
        },
        "workerSize": {
            "type": "string",
            "allowedValues": [
                "0",
                "1",
                "2"
            ],
            "defaultValue": "0"
        }
    }

}